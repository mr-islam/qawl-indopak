#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/is-ci/1.2.1/node_modules/is-ci/bin.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/is-ci/1.2.1/node_modules/is-ci/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/is-ci/1.2.1/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/is-ci/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../is-ci/1.2.1/node_modules/is-ci/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/is-ci/1.2.1/node_modules/is-ci/bin.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/is-ci/1.2.1/node_modules/is-ci/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/is-ci/1.2.1/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/is-ci/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../is-ci/1.2.1/node_modules/is-ci/bin.js" "$@"
  ret=$?
fi
exit $ret
