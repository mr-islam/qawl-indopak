#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/4.0.1/node_modules/esprima/bin/esvalidate.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/4.0.1/node_modules/esprima/bin/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/4.0.1/node_modules/esprima/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/4.0.1/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../esprima/4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/4.0.1/node_modules/esprima/bin/esvalidate.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/4.0.1/node_modules/esprima/bin/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/4.0.1/node_modules/esprima/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/4.0.1/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/esprima/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../esprima/4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
  ret=$?
fi
exit $ret
