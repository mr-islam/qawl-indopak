#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/3.12.0/node_modules/js-yaml/bin/js-yaml.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/3.12.0/node_modules/js-yaml/bin/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/3.12.0/node_modules/js-yaml/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/3.12.0/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../js-yaml/3.12.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/3.12.0/node_modules/js-yaml/bin/js-yaml.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/3.12.0/node_modules/js-yaml/bin/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/3.12.0/node_modules/js-yaml/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/3.12.0/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/js-yaml/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../js-yaml/3.12.0/node_modules/js-yaml/bin/js-yaml.js" "$@"
  ret=$?
fi
exit $ret
