#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/nopt.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../nopt/1.0.10/node_modules/nopt/bin/nopt.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/nopt.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/nopt/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../nopt/1.0.10/node_modules/nopt/bin/nopt.js" "$@"
  ret=$?
fi
exit $ret
