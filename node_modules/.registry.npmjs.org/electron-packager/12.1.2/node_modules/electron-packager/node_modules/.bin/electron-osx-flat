#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/0.4.10/node_modules/electron-osx-sign/bin/electron-osx-flat.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/0.4.10/node_modules/electron-osx-sign/bin/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/0.4.10/node_modules/electron-osx-sign/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/0.4.10/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../electron-osx-sign/0.4.10/node_modules/electron-osx-sign/bin/electron-osx-flat.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/0.4.10/node_modules/electron-osx-sign/bin/electron-osx-flat.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/0.4.10/node_modules/electron-osx-sign/bin/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/0.4.10/node_modules/electron-osx-sign/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/0.4.10/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/electron-osx-sign/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../electron-osx-sign/0.4.10/node_modules/electron-osx-sign/bin/electron-osx-flat.js" "$@"
  ret=$?
fi
exit $ret
