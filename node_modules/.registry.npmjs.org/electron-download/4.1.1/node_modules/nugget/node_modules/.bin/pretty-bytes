#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/pretty-bytes/1.0.4/node_modules/pretty-bytes/cli.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/pretty-bytes/1.0.4/node_modules/pretty-bytes/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/pretty-bytes/1.0.4/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/pretty-bytes/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../pretty-bytes/1.0.4/node_modules/pretty-bytes/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/pretty-bytes/1.0.4/node_modules/pretty-bytes/cli.js/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/pretty-bytes/1.0.4/node_modules/pretty-bytes/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/pretty-bytes/1.0.4/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/pretty-bytes/node_modules:/mnt/c/Code/indopak-qawl/node_modules/.registry.npmjs.org/node_modules:/mnt/c/Code/indopak-qawl/node_modules:/mnt/c/Code/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../pretty-bytes/1.0.4/node_modules/pretty-bytes/cli.js" "$@"
  ret=$?
fi
exit $ret
